<%- include("partials/header") %>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <h3>Title</h3>
      <label for="formGroupExampleInput"></label>
      <input type="text" class="form-control" id="formGroupExampleInput" name="inputTitle" placeholder="Enter Title">
    </div>

    <div class="form-group">
      <h3>Post</h3>
      <label for="exampleFormControlTextarea1"></label>
      <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" name="inputPost" placeholder="Write your post"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Publish</button>
 </form>

<%- include("partials/footer") %>
